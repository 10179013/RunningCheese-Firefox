			"group": "重定向"
		},
		{
			"enable": true,
			"name": "奶酪网站去跳转",
			"ruleType": "redirect",
			"matchType": "regexp",
			"pattern": "^https?:\\/\\/www\\.runningcheese\\.com\\/go\\/\\?url=(.*)$",
			"exclude": "",
			"isFunction": false,
			"action": "redirect",
			"to": "$1",
			"group": "重定向"
		}
	],
	"sendHeader": [],
	"receiveHeader": []
}
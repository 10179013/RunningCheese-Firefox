			"group": "重定向"
		},
		{
			"enable": true,
			"name": "百度网盘去跳转",
			"ruleType": "redirect",
			"matchType": "regexp",
			"pattern": "^https?:\\/\\/.+\\?url=((https?:\\/\\/)?pan\\.baidu\\.com\\/s\\/.+)$",
			"exclude": "",
			"isFunction": false,
			"action": "redirect",
			"to": "$1",
			"group": "重定向"
		},
		{
			"enable": true,
			"name": "Youtube去跳转",
			"ruleType": "redirect",
			"matchType": "regexp",
			"pattern": "^https?:\\/\\/www\\.youtube\\.com\\/redirect\\?redir_token(.*)&q=(.*)$",
			"exclude": "",
			"isFunction": false,

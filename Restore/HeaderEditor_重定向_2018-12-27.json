			"action": "redirect",
			"to": "https://fonts.geekzu.org/$1",
			"group": "重定向"
		},
		{
			"enable": true,
			"name": "google ajax >> 极客族",
			"ruleType": "redirect",
			"matchType": "regexp",
			"pattern": "^https?:\\/\\/ajax\\.googleapis\\.com\\/(.*)$",
			"exclude": "",
			"isFunction": false,
			"action": "redirect",
			"to": "https://gapis.geekzu.org/ajax/$1",
			"group": "重定向"
		},
		{
			"enable": true,
